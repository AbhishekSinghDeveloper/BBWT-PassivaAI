{
    "version": "0.8",

    "index_id": "bbwt3-s3",

	"index_uri": "s3://bb.quickwit-test/indexes/bbwt3-s3",

    "doc_mapping": {
        "mode": "dynamic",
        "field_mappings": [
            {
                "name": "Level",
                "type": "text",
                "tokenizer": "default",
                "record": "position",
                "stored": true,
                "fast": true
            },
            {
                "name": "MessageTemplate",
                "type": "json",
                "tokenizer": "default",
                "record": "position",
                "stored": true
            },
            {
                "name": "Timestamp",
                "type": "datetime",
                "fast": true,
                "input_formats": [ "iso8601" ],
                "fast_precision": "seconds"
            }
        ],
        "timestamp_field": "Timestamp"
    },

    "search_settings": {
        "default_search_fields": [ "Level", "Timestamp", "MessageTemplate.Text"]
    },
    "indexing_settings": {
        "commit_timeout_secs": 10
    }
}